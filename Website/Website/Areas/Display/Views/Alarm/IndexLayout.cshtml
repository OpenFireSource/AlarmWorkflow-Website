<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - AlarmWorkflow.Website.Reports</title>

    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="@Styles.Url("~/Content/app/area-alarm-indexlayout.css")" />

    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/app/common")
    @RenderSection("scripts", required: false)
</head>
<body ng-app="alarmApp">
    <div ng-controller="alarmCtrl">
        @RenderBody()
    </div>
</body>
</html>
